require 'csv'    

namespace :games do
  task seed_games :evironment do
    

CSV.foreach("ign.csv", :headers => true) do |row|
  
  games.create!(
  score_phase: row[0]
  title: row[1]   
  platform: row[2]
  genre: row[3]
  release_year: row[4]
  release_month: row[5]
  release_day: row[6]
  )
  
    
end

 


CSV.foreach("score.csv", :headers => true) do |row|

 score.create!(
 title: row[0]
 score: row[1]
 )
 
end
